{
  "expo": {
    "name": "Trans AI Journaller",
    "slug": "trans-ai-journaller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.t2a.journaller",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.t2a.journaller",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.t2a.journaller",
    "plugins": ["expo-router"],
    "extra": {
      "apiUrl": "https://www.thoughts2action.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d26e917a-19ac-4ceb-be9b-24297318f855"
      }
    },
    "owner": "avini"
  }
}
